<Query>
<![CDATA[
SELECT a.单位,ifnull(b.KPI,100) KPI
FROM
(
	SELECT 单位
	FROM 新单位信息
	WHERE 单位类型 = '厅店'
	AND 营业状态 = '营业'
	AND 单位 ='${单位}'
) a

LEFT JOIN
(
	SELECT unit,kpi
	FROM kpi_unit
	WHERE ym = '${月份}'
) b ON a.单位 =b.unit]]></Query>
</TableData>
