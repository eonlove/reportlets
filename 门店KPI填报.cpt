<Query>
<![CDATA[
SELECT name_in_company,department_trans,b.kpi
FROM selectroster_tmp a
LEFT JOIN 门店个人KPI b ON a.name_in_company = b.nickname AND a.`month` = b.ym
WHERE department_trans = '${单位}'
AND `month` = '${月份}'
AND position_trans NOT IN ('店长','承包人')]]></Query>
