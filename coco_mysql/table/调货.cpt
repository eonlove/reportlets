<TableData name="推荐出货门店" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[coco]]></DatabaseName>
</Connection>
<Query>
<![CDATA[
SELECT *
FROM
(
	SELECT tb1.*,tb2.最优库存,tb1.当前库存-tb2.最优库存 AS 可出货量,
	row_number() over(PARTITION BY 条形码 ORDER BY tb1.当前库存-tb2.最优库存 DESC) AS rn
	FROM
	(
	SELECT 门店名称,品名,条形码,库存数量 AS 当前库存
	FROM zl_kucun
	WHERE 导入时间 = (SELECT MAX(导入时间) FROM zl_kucun)
	) tb1

	LEFT JOIN

	(
		SELECT 门店,商品条码,近30天销量*0.35 AS 最优库存
		FROM sales_summary_unit
	) tb2 ON tb1.门店名称 = tb2.门店 AND tb1.条形码 = tb2.商品条码
	WHERE tb1.当前库存-tb2.最优库存 >0
) a
WHERE rn = 1]]></Query>
</TableData>
<TableData name="可出货门店数/总量" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[coco]]></DatabaseName>
</Connection>
<Query>
<![CDATA[
SELECT 条形码,COUNT(门店名称) AS 可出货门店数,SUM(可出货量) AS 可出货总量
FROM
(
	SELECT tb1.*,tb2.最优库存,tb1.当前库存-tb2.最优库存 AS 可出货量,
	row_number() over(PARTITION BY 条形码 ORDER BY tb1.当前库存-tb2.最优库存 DESC) AS rn
	FROM
	(
	SELECT 门店名称,品名,条形码,库存数量 AS 当前库存
	FROM zl_kucun
	WHERE 导入时间 = (SELECT MAX(导入时间) FROM zl_kucun)
	) tb1

	LEFT JOIN

	(
		SELECT 门店,商品条码,近30天销量*0.35 AS 最优库存
		FROM sales_summary_unit
	) tb2 ON tb1.门店名称 = tb2.门店 AND tb1.条形码 = tb2.商品条码
	WHERE tb1.当前库存-tb2.最优库存 >0
) a
GROUP BY 条形码]]></Query>
</TableData>
<TableData name="需进货门店量/货物量" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[coco]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT 条形码,COUNT(*) AS 需进货门店数,SUM(推荐进货量) AS 需进货总量
FROM
(
	###允许进货清单
	SELECT a.*,
	IF(b.条形码 IS NULL,0,1) AS 本店是否有售,
	IFNULL(c.在售门店数,0) AS 在售门店数,
	IFNULL(e.库存数量,0) AS 库存数量,
	IFNULL(d.`近30天销量`/30,0) AS 30日均销量,
	IFNULL(d.近14日销量,0) AS 近14日销量,
	
	CASE 
		WHEN IFNULL(e.库存数量,0) != 0 AND IFNULL(d.`近30天销量`,0) = 0 THEN 999 
		WHEN IFNULL(e.库存数量,0) = 0 AND IFNULL(d.`近30天销量`,0) = 0 THEN 0
		ELSE IFNULL(e.库存数量,0)/IFNULL(d.`近30天销量`,0)*30 
	END AS 30日均dos,
	
	CASE
		WHEN IFNULL(e.库存数量,0) = 0 THEN IFNULL(d.近14日销量,0)
		ELSE f.case2进货量
	END AS 推荐进货量

	FROM
	(
		SELECT department AS 门店,品名,条形码
		FROM department_info,(SELECT 品名,条形码 FROM dim_goods WHERE 自身包装种类 != '大包装') t
	) a

	LEFT JOIN

	(
	###本店是否有售
		SELECT DISTINCT 品名,条形码,门店名称
		FROM ZL_kucun 
		WHERE 导入时间 = (SELECT MAX(导入时间) FROM zl_kucun)

		UNION

		SELECT DISTINCT 小包装商品名称,小包装码,门店
		FROM spls_to_xbz
		WHERE 交易时间 >= CONCAT(DATE_FORMAT((CURDATE() + INTERVAL -(1) MONTH),'%Y-%m'),'-01')
	) b ON a.门店 = b.门店名称 AND a.条形码 = b.条形码
	
	LEFT JOIN
	
	(
		SELECT 条形码,COUNT(*) AS 在售门店数
		FROM
		(
			SELECT DISTINCT 条形码,门店名称
			FROM ZL_kucun 
			WHERE 导入时间 = (SELECT MAX(导入时间) FROM zl_kucun)

			UNION

			SELECT DISTINCT 小包装码,门店
			FROM spls_to_xbz
			WHERE 交易时间 >= CONCAT(DATE_FORMAT((CURDATE() + INTERVAL -(1) MONTH),'%Y-%m'),'-01')
		) t
		GROUP BY 条形码
	) c ON a.条形码 = c.条形码

	LEFT JOIN sales_summary_unit d ON a.门店=d.门店 AND a.条形码 = d.商品条码
	
	LEFT JOIN 
	
	(
		SELECT *
		FROM ZL_kucun 
		WHERE 导入时间 = (SELECT MAX(导入时间) FROM zl_kucun)
	) e ON a.条形码 = e.条形码 AND a.门店 = e.门店名称
	
	LEFT JOIN
	(
		SELECT t.*,t1.近90日销量,
		t.分货占比*IFNULL(t1.近90日销量,0)*0.4 AS case2进货量
		FROM fhzb t
		LEFT JOIN
		(
			SELECT 商品条码,近90日销量
			FROM sales_summary
		) t1 ON t.小包装码 = t1.商品条码
	) f ON a.门店 = f.门店 AND a.条形码 = f.小包装码
) tbb
WHERE 在售门店数>=4 AND 30日均dos<7
GROUP BY 条形码]]></Query>
</TableData>
