</Connection>
<Query>
<![CDATA[SELECT 条形码,COUNT(*) AS 在售门店数
FROM
(
	SELECT DISTINCT 条形码,门店名称
	FROM ZL_kucun 
	WHERE 导入时间 = (SELECT MAX(导入时间) FROM zl_kucun)

