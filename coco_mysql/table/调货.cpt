<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[coco]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT 一级分类,二级分类,三级分类,四级分类,品名,
条形码,规格,是否有大包装,大包装码,
t1.*
FROM dim_goods t
LEFT JOIN 
(
	SELECT
	   tb.门店,
	  `tb`.`商品条码`     AS `商品条码`,
	   tb.销量7 AS 近14日销量,
	  `tb`.`月销量6`       AS `150-180天销量`,
	  `tb`.`月销量5`       AS `120-150天销量`,
	  `tb`.`月销量4`       AS `90-120天销量`,
	  `tb`.`月销量3`       AS `60-90天销量`,
	  `tb`.`月销量2`       AS `30-60天销量`,
	  `tb`.`月销量1`       AS `近30天销量`,
	  `tb`.`近7日均销量` AS `近7日均销量`,
	  `tb`.`本月销量`     AS `本月销量`,
	  `tb`.`上月销量`     AS `上月销量`,
	  IF((IF((IF((IF((IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`) > `tb`.`月销量4`),IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`),`tb`.`月销量4`) > `tb`.`月销量3`),IF((IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`) > `tb`.`月销量4`),IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`),`tb`.`月销量4`),`tb`.`月销量3`) > `tb`.`月销量2`),IF((IF((IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`) > `tb`.`月销量4`),IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`),`tb`.`月销量4`) > `tb`.`月销量3`),IF((IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`) > `tb`.`月销量4`),IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`),`tb`.`月销量4`),`tb`.`月销量3`),`tb`.`月销量2`) > `tb`.`月销量1`),IF((IF((IF((IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`) > `tb`.`月销量4`),IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`),`tb`.`月销量4`) > `tb`.`月销量3`),IF((IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`) > `tb`.`月销量4`),IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`),`tb`.`月销量4`),`tb`.`月销量3`) > `tb`.`月销量2`),IF((IF((IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`) > `tb`.`月销量4`),IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`),`tb`.`月销量4`) > `tb`.`月销量3`),IF((IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`) > `tb`.`月销量4`),IF((`tb`.`月销量6` > `tb`.`月销量5`),`tb`.`月销量6`,`tb`.`月销量5`),`tb`.`月销量4`),`tb`.`月销量3`),`tb`.`月销量2`),`tb`.`月销量1`) AS `半年最大月销量`,
	  (((((((((`tb`.`月销量1` + `tb`.`月销量2`) + `tb`.`月销量3`) + `tb`.`月销量4`) + `tb`.`月销量5`) + `tb`.`月销量6`) / 180) * 7) * 1.2) / 2) AS `周最大销量一半`
	FROM (SELECT
		zl_spls.门店,
		`zl_spls`.`商品条码` AS `商品条码`,
		SUM(IF((LEFT(`zl_spls`.`交易时间`,10) BETWEEN (CURDATE() + INTERVAL -(14) DAY) AND CURDATE()),`zl_spls`.`数量`,0)) AS `销量7`,
		SUM(IF((LEFT(`zl_spls`.`交易时间`,10) BETWEEN (CURDATE() + INTERVAL -(180) DAY) AND (CURDATE() + INTERVAL -(150) DAY)),`zl_spls`.`数量`,0)) AS `月销量6`,
		SUM(IF((LEFT(`zl_spls`.`交易时间`,10) BETWEEN (CURDATE() + INTERVAL -(150) DAY) AND (CURDATE() + INTERVAL -(120) DAY)),`zl_spls`.`数量`,0)) AS `月销量5`,
		SUM(IF((LEFT(`zl_spls`.`交易时间`,10) BETWEEN (CURDATE() + INTERVAL -(120) DAY) AND (CURDATE() + INTERVAL -(90) DAY)),`zl_spls`.`数量`,0)) AS `月销量4`,
		SUM(IF((LEFT(`zl_spls`.`交易时间`,10) BETWEEN (CURDATE() + INTERVAL -(90) DAY) AND (CURDATE() + INTERVAL -(60) DAY)),`zl_spls`.`数量`,0)) AS `月销量3`,
		SUM(IF((LEFT(`zl_spls`.`交易时间`,10) BETWEEN (CURDATE() + INTERVAL -(60) DAY) AND (CURDATE() + INTERVAL -(30) DAY)),`zl_spls`.`数量`,0)) AS `月销量2`,
		SUM(IF((LEFT(`zl_spls`.`交易时间`,10) BETWEEN (CURDATE() + INTERVAL -(30) DAY) AND CURDATE()),`zl_spls`.`数量`,0)) AS `月销量1`,
		(SUM(IF((LEFT(`zl_spls`.`交易时间`,10) BETWEEN (CURDATE() + INTERVAL -(8) DAY) AND (CURDATE() + INTERVAL -(1) DAY)),`zl_spls`.`数量`,0)) / 7) AS `近7日均销量`,
		SUM(IF((LEFT(`zl_spls`.`交易时间`,10) BETWEEN CONCAT(LEFT(CURDATE(),7),'-01') AND CONCAT(LAST_DAY(CONCAT(LEFT(CURDATE(),7),'-01')),' 23:59:59')),`zl_spls`.`数量`,0)) AS `本月销量`,
		SUM(IF((LEFT(`zl_spls`.`交易时间`,10) BETWEEN CONCAT(LEFT((CURDATE() + INTERVAL -(1) MONTH),7),'-01') AND CONCAT(LAST_DAY(CONCAT(LEFT((CURDATE() + INTERVAL -(1) MONTH),7),'-01')),' 23:59:59')),`zl_spls`.`数量`,0)) AS `上月销量`
	      FROM spls_to_xbz AS `zl_spls`
	      WHERE (LEFT(`zl_spls`.`交易时间`,10) >= (CURDATE() + INTERVAL - (180)DAY))
	      GROUP BY zl_spls.门店,`zl_spls`.`商品条码`) `tb`
)
 t1 ON t.条形码 = t1.商品条码
 
WHERE 自身包装种类 != '大包装'
AND t1.门店 IS NOT NULL
AND 1=1 ${IF(len(FL1)==0,"","and 一级分类 = '"+FL1+"'")}
AND 1=1 ${IF(len(FL2)==0,"","and 二级分类 = '"+FL2+"'")}
AND 1=1 ${IF(len(FL3)==0,"","and 三级分类 = '"+FL3+"'")}
AND 1=1 ${IF(len(BYXL)==0,"","and t1.本月销量 <= '"+BYXL+"'")}
AND 1=1 ${IF(len(BYXL2)==0,"","and t1.本月销量 >= '"+BYXL2+"'")}]]></Query>
</TableData>
