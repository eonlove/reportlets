<TableData name="在售门店数" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[coco]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT t1.条形码,COUNT(*) AS 在售门店数
FROM

(
	SELECT  department,条形码
	FROM department_info,dim_goods
) t1 

LEFT JOIN (SELECT * FROM ZL_kucun WHERE 导入时间 = (SELECT MAX(导入时间) FROM zl_kucun)) zk ON t1.条形码 = zk.条形码 AND t1.department = zk.门店名称


LEFT JOIN

(
	SELECT	
	门店,
	`zl_spls`.`商品条码` AS `条形码`,
	SUM(IF((LEFT(`zl_spls`.`交易时间`,7) = DATE_FORMAT(CURDATE(),'%Y-%m')),`zl_spls`.`数量`,0)) AS `本月销量`,
	SUM(IF((LEFT(`zl_spls`.`交易时间`,7) = DATE_FORMAT((CURDATE() + INTERVAL -(1) MONTH),'%Y-%m')),`zl_spls`.`数量`,0)) AS `上月销量`
      FROM (
	     
	      SELECT   t.门店,
		      `t`.`交易时间`        AS `交易时间`,
		      `t1`.`大转小名称`    AS `小包名称`,
		      `t1`.`小包装码`       AS `商品条码`,
		      `t`.`应收金额`        AS `应收金额`,
		      (`t`.`数量` * `t1`.`包装数量`) AS `数量`
		    FROM `zl_spls` `t`
		    LEFT JOIN `dim_goods` `t1` ON `t`.`商品条码` = `t1`.`条形码`
		    WHERE `t1`.`自身包装种类` = '大包装'
	    
		    UNION ALL 
		    
		    SELECT
		    t.门店,
	       `t`.`交易时间`         AS `交易时间`,
	       `t1`.`品名`              AS `品名`,
	       `t`.`商品条码`         AS `商品条码`,
	       `t`.`应收金额`         AS `应收金额`,
	       `t`.`数量`               AS `数量`
	     FROM `zl_spls` `t`
		LEFT JOIN `dim_goods` `t1`
		  ON `t`.`商品条码` = `t1`.`条形码`
	     WHERE `t1`.`自身包装种类` <> '大包装'
	     
     ) `zl_spls`
      WHERE (LEFT(`zl_spls`.`交易时间`,10) >= (CURDATE() + INTERVAL - (180)DAY)) 
      GROUP BY 门店,`zl_spls`.`商品条码`
) t2 ON t1.条形码 = t2.条形码 AND t1.department = t2.门店

WHERE IFNULL(zk.`库存数量`,0)+IFNULL(本月销量,0)+IFNULL(上月销量,0) > 0 
GROUP BY t1.条形码]]></Query>
</TableData>
</TableDataMap>
