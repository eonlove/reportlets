<TableData name="采购金额" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[coco]]></DatabaseName>
</Connection>
<Query>
<![CDATA[
SELECT tb.*,tb1.采购次数,tb2.供应商数量,tb3.季度采购均价,
tb4.本批采购金额,
tb4.上批采购金额,
tb4.大上批采购金额
FROM
(
	SELECT 小包装码 AS 条形码,大转小名称 AS 品名
	FROM ZL_CRKMX t
	LEFT JOIN dim_goods t1 ON t.条形码 = t1.`条形码`
	WHERE 自身包装种类 = '大包装'
	GROUP BY t.条形码

	UNION ALL

	SELECT t.条形码,t.品名
	FROM ZL_CRKMX t
	LEFT JOIN dim_goods t1 ON t.条形码 = t1.`条形码`
	WHERE 自身包装种类 != '大包装'
	GROUP BY t.条形码
) tb

LEFT JOIN

(
	SELECT 小包装码,SUM(采购次数) AS 采购次数
	FROM
	(
		SELECT tb1.`小包装码`,tb.采购次数,tb1.`自身包装种类`
		FROM
		(
			SELECT 条形码,COUNT(*) AS 采购次数
			FROM
			(
				SELECT 条形码,单据编号
				FROM cus_cgmx
				GROUP BY 条形码,单据编号
			) t
			GROUP BY 条形码
		) tb

		LEFT JOIN dim_goods tb1 ON tb.条形码 = tb1.条形码
		WHERE 自身包装种类 = '大包装'


		UNION ALL


		SELECT tb.*,tb1.`自身包装种类`
		FROM
		(
			SELECT 条形码,COUNT(*) AS 采购次数
			FROM
			(
				SELECT 条形码,单据编号
				FROM cus_cgmx
				GROUP BY 条形码,单据编号
			) t
			GROUP BY 条形码
		) tb

		LEFT JOIN dim_goods tb1 ON tb.条形码 = tb1.条形码
		WHERE 自身包装种类 != '大包装'
	) tbb
	GROUP BY 小包装码
) tb1 ON tb.条形码 = tb1.小包装码


LEFT JOIN

(
	SELECT 小包装码,COUNT(*) AS 供应商数量
	FROM
	(
		SELECT t1.`小包装码`,t.采购单供应商
		FROM
		(
			SELECT 条形码,采购单供应商
			FROM ZL_CGMX
			GROUP BY 条形码,采购单供应商
		) t

		LEFT JOIN dim_goods t1 ON t.条形码=  t1.条形码
		WHERE t1.`自身包装种类` = '大包装'

		UNION ALL

		SELECT t.*
		FROM
		(
			SELECT 条形码,采购单供应商
			FROM ZL_CGMX
			GROUP BY 条形码,采购单供应商
		) t

		LEFT JOIN dim_goods t1 ON t.条形码=  t1.条形码
		WHERE t1.`自身包装种类` != '大包装'
	) t
	GROUP BY 小包装码
) tb2 ON tb.条形码 = tb2.小包装码

LEFT JOIN

(

	SELECT 
	t1.条形码,SUM(t1.采购金额)/SUM(t1.数量) AS 季度采购均价
	FROM
	(
		SELECT 单据编号,小包装码 AS 条形码,大转小名称 AS 品名,t.采购金额,包装数量*(IFNULL(t.数量,0)+IFNULL(t.赠送数量,0)) AS 数量
		FROM ZL_cgmx t
		LEFT JOIN dim_goods t1 ON t.条形码 =t1.条形码
		WHERE t1.自身包装种类 = '大包装'

		UNION ALL


		SELECT 单据编号,t.条形码,t.品名,t.采购金额,IFNULL(t.数量,0)+IFNULL(t.赠送数量,0) AS 数量
		FROM ZL_cgmx t
		LEFT JOIN dim_goods t1 ON t.条形码 =t1.条形码
		WHERE t1.自身包装种类 != '大包装'
	) t1

	LEFT JOIN

	(
		SELECT 单据编号,LEFT(业务发生日期,10) AS 日期
		FROM ZL_CRKMX 
		GROUP BY 单据编号,LEFT(业务发生日期,10)
	) t2 ON t1.单据编号 = t2.单据编号

	WHERE DATEDIFF(CURDATE(),日期) <= 90
	GROUP BY t1.条形码
) tb3 ON tb.条形码 = tb3.条形码

LEFT JOIN

(
	SELECT 条形码,
	SUM(IF(rn=1,批次采购金额,0)) AS 本批采购金额,
	SUM(IF(rn=2,批次采购金额,0)) AS 上批采购金额,
	SUM(IF(rn=3,批次采购金额,0)) AS 大上批采购金额
	FROM
	(
		SELECT 条形码,单据编号,
		SUM(采购金额)/SUM(数量) AS 批次采购金额,
		row_number() over(PARTITION BY 条形码 ORDER BY 单据编号 DESC) AS rn

		FROM
		(
			SELECT 单据编号,小包装码 AS 条形码,大转小名称 AS 品名,t.采购金额,包装数量*(IFNULL(t.数量,0)+IFNULL(t.赠送数量,0)) AS 数量
			FROM ZL_cgmx t
			LEFT JOIN dim_goods t1 ON t.条形码 =t1.条形码
			WHERE t1.自身包装种类 = '大包装'

			UNION ALL


			SELECT 单据编号,t.条形码,t.品名,t.采购金额,IFNULL(t.数量,0)+IFNULL(t.赠送数量,0) AS 数量
			FROM ZL_cgmx t
			LEFT JOIN dim_goods t1 ON t.条形码 =t1.条形码
			WHERE t1.自身包装种类 != '大包装'
		) tb

		GROUP BY 条形码,单据编号
	) v
	GROUP BY 条形码
) tb4 ON tb.条形码 = tb4.条形码]]></Query>
</TableData>
