<TableData name="日销量max本月" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="门店"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[coco]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT 商品条码,LEFT(交易时间,10) AS 日期,SUM(数量) AS 数量
FROM (
SELECT
`t`.`交易时间`        AS `交易时间`,
`t1`.`大转小名称`    AS `小包名称`,
`t1`.`小包装码`       AS `商品条码`,
`t`.`应收金额`        AS `应收金额`,
(`t`.`数量` * `t1`.`包装数量`) AS `数量`
FROM (`zl_spls` `t`
LEFT JOIN `dim_goods` `t1`
 ON ((`t`.`商品条码` = `t1`.`条形码`)))
WHERE (`t1`.`自身包装种类` = '大包装')
 AND 1=1 ${IF(len(门店)==0,"","and t.门店 ='"+门店+"'")}


UNION ALL 


SELECT
`t`.`交易时间`         AS `交易时间`,
`t1`.`品名`              AS `品名`,
`t`.`商品条码`         AS `商品条码`,
`t`.`应收金额`         AS `应收金额`,
`t`.`数量`               AS `数量`
FROM (`zl_spls` `t`
LEFT JOIN `dim_goods` `t1`
  ON ((`t`.`商品条码` = `t1`.`条形码`)))
WHERE (`t1`.`自身包装种类` <> '大包装')
 AND 1=1 ${IF(len(门店)==0,"","and t.门店 ='"+门店+"'")}
)zl_spls
WHERE LEFT(交易时间,7) = DATE_FORMAT(CURDATE(),'%Y-%m')
GROUP BY 商品条码,LEFT(交易时间,10)]]></Query>
