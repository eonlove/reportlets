<Query>
<![CDATA[SELECT 条形码,品名,SUM(库存数量) AS 数量,SUM(成本总额) AS 成本总额
FROM zl_kucun
where 1=1 ${if(len(门店)==0,"","and 门店名称 = '"+门店+"'")}
and 导入时间 = (select max(导入时间) from zl_kucun)
GROUP BY 条形码]]></Query>
